" class=\"my-8 mr-8 border border-gray-300 py-2 px-4 rounded bg-blue-600 text-white\">Connexion</button>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n\n    import { mapActions, mapGetters }  from 'vuex';\n\n    export default {\n\n        computed: {\n            ...mapGetters({\n                connect: 'getConnected',\n                token: 'getToken'\n            })\n        },\n        methods: {\n            ...mapActions(['login']),\n\n            callLogin(){\n                let email = document.querySelector('#email').value;\n                let pass = document.querySelector('#pass').value;\n\n                let data = {\n                    \"email\": email,\n                    \"password\": pass,\n                };\n\n                this.login(data);\n            },\n\n            cancelModal(nomModal){\n                let modal = document.querySelector(nomModal);\n                modal.classList.add('hidden');\n            },\n\n            showModal(nomModal){\n                    let modal = document.querySelector(nomModal);\n                    modal.classList.remove('hidden');\n            },\n        }\n    }\n</script>\n\n<style lang=\"scss\" scoped>\n\n</style>"],"sourceRoot":""}]}